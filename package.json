{
  "name": "svc-ather-collector",
  "version": "0.0.1",
  "description": "Collector to read from Gcp pub/sub and write to Kafka",
  "main": "dist/main.js",
  "author": "VIMANA",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/systeminsights/svc-ather-collector/issues"
  },
  "homepage": "https://github.com/systeminsights/svc-ather-collector#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/systeminsights/svc-ather-collector.git"
  },
  "scripts": {
    "start": "ELASTIC_APM_ACTIVE=false babel-node src/main.js",
    "generate-probe": "babel-node scripts/parseCSVtoJSON.js",
    "debug": "ELASTIC_APM_ACTIVE=false babel-node scripts/gcpConsumer.js",
    "debug-subscriber": "babel-node src/gcpSubscriber/index.js",
    "debug-publisher": "babel-node scripts/gcpProducer.js",
    "start-dev-staging": "VI_MODE=dev VI_ES_HOST=s-elasticsearch-1.use1.systeminsights.com npm run start",
    "test": "ELASTIC_APM_ACTIVE=false mocha --reporter dot --recursive test",
    "eslint": "eslint ./src ./test",
    "eslint-fix": "eslint ./src ./test --fix",
    "tc:version": "tc-version-app",
    "tc:eslint": "tc-eslint",
    "tc:test_pre": "docker-compose -f teamcity-services.yml up -d",
    "tc:coverage": "tc-coverage-mocha",
    "tc:test_post": "docker-compose -f teamcity-services.yml down",
    "tc:build": "tc-build"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitmsg-jira",
      "pre-commit": "tc-eslint"
    }
  },
  "dependencies": {
    "@google-cloud/pubsub": "1.1.5",
    "event-loop-stats": "1.2.0",
    "node-metric-registry": "1.0.24",
    "node-microservice": "3.5.364",
    "ramda": "0.26.1",
    "rxjs": "6.5.3",
    "vi-kafka-stream-client": "1.0.209"
  },
  "devDependencies": {
    "@babel/node": "7.7.0",
    "babel-preset-vi-node": "1.0.90",
    "csvtojson": "2.0.10",
    "eslint-config-vi-node": "1.2.124",
    "chai": "4.2.0",
    "husky": "3.0.9",
    "mocha": "6.2.2",
    "sinon": "7.5.0",
    "sinon-chai": "3.3.0"
  }
}
