{
  "name": "svc-ather-collector",
  "version": "0.0.1",
  "description": "Collector to read from Gcp pub/sub and write to Kafka",
  "main": "dist/main.js",
  "author": "VIMANA",
  "license": "ISC",
  "bugs": {
    "deviceRegistryUrl": "https://github.com/systeminsights/svc-ather-collector/issues"
  },
  "homepage": "https://github.com/systeminsights/svc-ather-collector#readme",
  "repository": {
    "type": "git",
    "deviceRegistryUrl": "git+https://github.com/systeminsights/svc-ather-collector.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitmsg-jira",
      "pre-commit": "tc-eslint"
    }
  },
  "scripts": {
    "start": "ELASTIC_APM_ACTIVE=false babel-node src/main.js",
    "generate-probe": "babel-node scripts/parseCSVtoJSON.js",
    "debug-gcp": "ELASTIC_APM_ACTIVE=false babel-node scripts/gcpConsumer.js",
    "debug-subscriber": "babel-node src/source/gcp/index.js",
    "debug-publisher": "babel-node scripts/gcpProducer.js",
    "create-legacy-component-version-config": "babel-node scripts/createLegacyComponentVersionConfig",
    "test": "test-mocha",
    "eslint": "eslint ./src ./test",
    "eslint-fix": "eslint ./src ./test --fix",
    "tc:eslint": "tc-eslint",
    "tc:coverage": "tc-coverage-mocha",
    "tc:build": "tc-build",
    "test-local": "mocha --recursive test"
  },
  "dependencies": {
    "@google-cloud/monitoring": "2.1.2",
    "@google-cloud/pubsub": "2.5.0",
    "avsc": "5.4.22",
    "axios": "0.20.0",
    "event-loop-stats": "1.3.0",
    "jwt-simple": "0.5.6",
    "long": "4.0.0",
    "node-metric-registry": "1.0.78",
    "node-microservice": "3.5.563",
    "node-rdkafka": "2.9.1",
    "ramda": "0.27.1",
    "rxjs": "6.6.3"
  },
  "devDependencies": {
    "@babel/node": "7.10.5",
    "babel-preset-vi-node": "1.0.151",
    "chai": "4.2.0",
    "choma": "1.2.1",
    "csvtojson": "2.0.10",
    "dotenv": "8.2.0",
    "eslint-config-vi-node": "1.2.219",
    "husky": "4.3.0",
    "mocha": "8.1.3",
    "sinon": "9.0.3",
    "sinon-chai": "3.5.0",
    "snake-case": "3.0.3",
    "nock": "13.0.4"
  },
  "mocha": {
    "require": [
      "@babel/register",
      "test/setup.js",
      "choma"
    ],
    "timeout": "5000"
  }
}
